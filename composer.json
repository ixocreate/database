{
    "name": "ixocreate/database",
    "description": "Database and Doctrine integration for IXOCREATE",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "doctrine/dbal": "^2.6",
        "doctrine/migrations": "^1.6",
        "doctrine/orm": "^2.6",
        "ixocreate/contract": "dev-develop",
        "ixocreate/entity": "dev-develop",
        "ixocreate/servicemanager": "dev-develop",
        "ixocreate/application": "dev-develop"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^1.0.1",
        "phpunit/phpunit": "^6.0.6"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Database\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IxocreateMisc\\Database\\": "tests/misc/",
            "IxocreateTest\\Database\\": "tests/src/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
