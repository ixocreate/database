{
    "name": "kiwi-suite/database",
    "description": "kiwi-suite/database",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thomas Payer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "doctrine/dbal": "^2.6",
        "doctrine/migrations": "^1.6",
        "doctrine/orm": "^2.6",
        "kiwi-suite/contract": "dev-develop",
        "kiwi-suite/entity": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.6",
        "friendsofphp/php-cs-fixer": "^2.5",
        "kiwi-suite/servicemanager": "dev-develop",
        "kiwi-suite/application": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Database\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IxocreateMisc\\Database\\": "tests/misc/",
            "IxocreateTest\\Database\\": "tests/src/"
        }
    }
}
